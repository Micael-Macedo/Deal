@model IEnumerable<Deal.Models.Acordo>
@{
    ViewData["Title"] = "AcordosPrestador";
    int PrestadorId = ViewBag.PrestadorId;
}

<h1>@ViewData["Title"]</h1>
<a asp-action="Home" asp-controller="Prestadores" asp-route-id="@PrestadorId" class="btn btn-info">Retornar a pagina inicial</a>
<table class="table">
    <thead>
        <tr>
            <th>
                Numero do acordo
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servico.Nome)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AcordoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkServico)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AcordoId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.AcordoId">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.AcordoId">Apagar</a> |
                <a asp-action="HomePage" asp-route-id="@item.AcordoId">PÃ¡gina Inicial</a>
            </td>
        </tr>
}
    </tbody>
</table>