@model Deal.Models.NotaPrestador

@{
    ViewData["Title"] = "Detalhe";
}

<h1>Detalhe</h1>

<div>
    <h4>Nota Prestador</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Avaliação
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Avaliacao)
        </dd>
        <dt class = "col-sm-2">
            Prestador
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Prestador.Nome)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.NotaPrestadorId">Editar</a> |
    <a asp-action="Index">Retornar</a>
</div>
