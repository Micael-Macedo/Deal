@model Deal.Models.NotaPrestador

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Você tem certeza que deseja cancelar este serviço?</h3>
<div>
    <h4>Nota Prestador</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Avaliação
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Avaliacao)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Prestador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Prestador.Nome)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="NotaPrestadorId" />
        <input type="submit" value="Apagar" class="btn btn-danger" /> |
        <a asp-action="Index">Retornar</a>
    </form>
</div>
