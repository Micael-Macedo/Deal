@model Deal.Models.NotaPrestador

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>

<h4>Nota Prestador</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="NotaPrestadorId" />
            <div class="form-group">
                <label class="control-label">Avaliação</label>
                <input asp-for="Avaliacao" class="form-control" />
                <span asp-validation-for="Avaliacao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Prestador</label>
                <select asp-for="FkPrestador" class="form-control" asp-items="ViewBag.FkPrestador"></select>
                <span asp-validation-for="FkPrestador" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Retornar</a>
</div>

