@model Deal.Models.ReporteCliente

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>ReporteCliente</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Motivo" class="control-label"></label>
                <input asp-for="Motivo" class="form-control" />
                <span asp-validation-for="Motivo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input hidden asp-for="FkCliente" class ="form-control" value="@ViewBag.FkCliente">
            </div>
            <div class="form-group">
                <input hidden asp-for="FkPrestador" class ="form-control" value="@ViewBag.FkPrestador">
            </div>
            <div class="form-group">
                <input hidden asp-for="FkServico" class ="form-control" value="@ViewBag.FkServico">
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



